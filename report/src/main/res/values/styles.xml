<resources>
    <style name="Theme.Report.Seekbar">
        <item name="android:progressDrawable">@drawable/seekbar_bg</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:max">5</item>
        <item name="android:indeterminate">false</item>
        <item name="android:maxHeight">6dp</item>
        <item name="android:minHeight">6dp</item>
        <item name="android:progress">0</item>
    </style>

    <style name="Theme.Report.DescriptionDetails">
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="Theme.Report.DescriptionDetailsFrame">
        <item name="android:visibility">gone</item>
        <item name="android:background">@color/background_dark_gray</item>
    </style>


</resources>
